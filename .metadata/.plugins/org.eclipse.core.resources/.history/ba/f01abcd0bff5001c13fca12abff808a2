package java0627;

import java.util.Scanner;

public class ex04_while문예제 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num;
		int sum = 0;
		do { System.out.println("숫자를 입력하세요");
			num = sc.nextInt();
			sum += num;
		} while (num <=100);
		
		System.out.print("합계");
		
		
	}

}
