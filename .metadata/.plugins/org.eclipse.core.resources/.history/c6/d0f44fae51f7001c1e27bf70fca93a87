package java0629;

public class ex04역삼각형 {

	public static void main(String[] args) {
		
		for(int i =1; i<=5 ; i++) {
			for(int k =1; k<=5-i; k++) {
				System.out.print(" ");
			}
			for(int j =1; j<=i ; j+=2) {
				System.out.print("*");
			}
			for(int k =1; k<=5-i; k++) {
				System.out.print(" ");
			
		}System.out.println();
		
		

	}
	
}
