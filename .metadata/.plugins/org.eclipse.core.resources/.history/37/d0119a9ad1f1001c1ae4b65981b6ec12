package java0622;

public class ex02산술연산자 {

	public static void main(String[] args) {
		int a = 100;
		int b = 200;
		
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b); //몫
		System.out.println(a%b); //나머지
		
		float num1 = 3.14f;
		float num2 = 16.999f;
		
		System.out.println(a+num1);
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		
	}

}
